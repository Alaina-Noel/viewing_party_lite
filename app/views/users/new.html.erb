<h1>Create New User</h1>
<%= form_with url: users_path, method: :post do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>
  <%= f.label :password, "Password" %>
  <%= f.text_field :password_field %>
  <%= f.submit "Create User" %>
<% end %>